{
  "name": "@cycle/most-adapter",
  "version": "1.0.0",
  "description": "Cycle.js Most.js Stream Adapter",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclejs/most-adapter.git"
  },
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "keywords": [
    "Cycle",
    "Cycle.js",
    "Most",
    "Most.js",
    "Adapter"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyclejs/most-adapter/issues"
  },
  "homepage": "https://github.com/cyclejs/most-adapter#readme",
  "typings": "lib/index.d.ts",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "most": "global:most"
  },
  "peerDependencies": {
    "@cycle/base": "*",
    "most": "^0.17.1",
    "most-subject": "2.x.x"
  },
  "devDependencies": {
    "@most/hold": "*",
    "@cycle/base": "*",
    "assert": "^1.3.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "most": "^0.17.1",
    "most-subject": "^2.0.0",
    "tslint": "^3.3.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.3",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "lint": "tslint -c tslint.json src/*.ts",
    "premocha": "npm run lib",
    "mocha": "mocha --compilers js:babel-register test",
    "test": "npm run lint && npm run mocha",
    "prelib": "rm -rf lib/ && mkdirp lib/ && typings install",
    "lib": "tsc",
    "predist": "rm -rf dist/ && mkdirp dist/ && npm run lib",
    "dist": "browserify -t browserify-shim -t babelify --exclude most lib/index.js -o dist/cycle-most-adapter.js",
    "postdist": "uglifyjs dist/cycle-most-adapter.js -o dist/cycle-most-adapter.min.js",
    "start": "npm install && npm prune",
    "prepublish": "npm run lib",
    "preversion": "npm run dist"
  }
}
