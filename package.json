{
  "name": "@cycle/most-adapter",
  "version": "1.0.0",
  "description": "Cycle.js Most.js Stream Adapter",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclejs/most-adapter.git"
  },
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "keywords": [
    "Cycle",
    "Cycle.js",
    "Most",
    "Most.js",
    "Adapter"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyclejs/most-adapter/issues"
  },
  "homepage": "https://github.com/cyclejs/most-adapter#readme",
  "browserify-shim": {
    "most": "global:most"
  },
  "dependencies": {
    "most": "^0.19.4",
    "most-subject": "^4.0.1"
  },
  "devDependencies": {
    "@cycle/base": "^2.1.0",
    "assert": "^1.3.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "lint": "eslint src/",
    "mocha": "mocha --compilers js:babel-register test",
    "test": "npm run lint && npm run mocha",
    "prelib": "rm -rf lib/ && mkdirp lib/",
    "lib": "babel src -d lib",
    "predist": "rimraf dist/ && mkdirp dist/ && npm run lib",
    "dist": "browserify -t browserify-shim -t babelify --exclude most src/index.js -o dist/cycle-most-adapter.js",
    "postdist": "uglifyjs dist/cycle-most-adapter.js -o dist/cycle-most-adapter.min.js",
    "start": "npm install && npm prune",
    "prepublish": "npm run lib",
    "preversion": "npm run dist"
  }
}
